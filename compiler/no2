#include<stdio.h>
#include<string.h>
//Write a C program that will take multiple lines as input and count the number of lines.
int valid( char ch2 ){
    if( (ch2 == '_') || (ch2 >= 'a' && ch2 <= 'z' ) || (ch2 >= 'A' && ch2 <= 'Z') || (ch2 >= '0' && ch2 <= '9') ){

        return 1;
    }
    else{
        return 0;
    }
}

int compare( char str[], char str2[] ){
    int i = 0;
    while( str[i] != '\0' || str2[i] != '\0' ){
        if( str[i] != str2[i] ){
            return 0;
        }
        else{
            i++;
        }
    }
    return 1;
}

int keyword( char str[] ){
char temp[8][50] = {"int", "if", "while", "return", "else", "for", "float", "char"};
    for( int i = 0; i < 8; i++ ){
        if( compare(str, temp[i]) ){
            return 1;
        }

    }
    return 0;

}

int identifier( char ch[] ){
    int i = 0;
    while( ch[i] != '\0' ){
        if( (ch[i] >= 'a' && ch[i] <= 'z') || (ch[i] >= 'a' && ch[i] <= 'z') ){
            i++;
        }
        else{
            return 0;
        }
    }
    return 1;
}

int main()
{
    char ch[500];
    gets(ch);

    int check = 0;
    char temp2[50][50];
    int n = 0;
    int i = 0, j = 0, count = 0;;

    for(i = 0; ch[i] != '\0'; i++ ){

         if( valid( ch[i] )){
           temp2[j][n] = ch[i];

           n++;
           }
           else{
               // printf("%c ", temp2[j][n]);
               if( n > 0){
                temp2[j][n] = '\0';
            n = 0;
            j++;
           count++;
               }

           }
    }
    if (n > 0) { // Last word
        temp2[j][n] = '\0';
        j++;
    }



   for( i = 0; i < j; i++ ){
    //keyword
    if( keyword(temp2[i])){
        printf( "%s : keyword\n", temp2[i]);
    }
    else if( identifier(temp2[i]) ){
        printf( "%s : Identifier\n", temp2[i]);
    }
    else{
        printf( "%s : (Not classified as keyword or identifier)\n", temp2[i]);
    }

   }

}
