#include<stdio.h>
#include<string.h>

int main()
{
    char ch[800];
    scanf("%[^$]", ch);
    int i = 0,j = 0, k = 0;

    while( ch[i] != '\0' ){
        //single
        if( ch[i] == '/' && ch[i+1] == '/' ){
            while( ch[i] != '\n' ){
                i++;
            }
        }
        // multiple line comment
        else if( ch[i] == '/' && ch[i+1] == '*' ){
                i+=2;
            while (!(ch[i] == '*' && ch[i + 1] == '/') && ch[i] != '\0') {
                i++;
            }
            if (ch[i] != '\0') {
                i += 2;
            }

        }

        else{
            printf("%c", ch[i] );
            i++;
        }
    }

}



